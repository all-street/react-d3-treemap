{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "jsx": "react",
        "typeRoots": [
            "./node_modules/@types"
        ],
        "types": [
            "jest",
            "node",
            "react",
            "react-dom"
        ]
    }
}
